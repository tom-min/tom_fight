N                        OSRdyTbl[ptcb->OSTCBY] |= ptcb->OSTCBBitX;
N                    }
N                }
N            }
N            ptcb = ptcb->OSTCBNext;                        /* Point at next TCB in TCB list                */
N            OS_EXIT_CRITICAL();
X            {OS_CPU_SR_Restore(cpu_sr);};
N        }
N    }
