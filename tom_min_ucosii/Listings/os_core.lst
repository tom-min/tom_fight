Nvoid  OS_TaskIdle (void *p_arg)
N{
N#if OS_CRITICAL_METHOD == 3u                     /* Allocate storage for CPU status register           */
X#if 3u == 3u                      
N    OS_CPU_SR  cpu_sr = 0u;
N#endif
N
N
N
